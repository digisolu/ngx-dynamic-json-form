{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-dynamic-json-form": {
      "projectType": "library",
      "root": "projects/ngx-dynamic-json-form",
      "sourceRoot": "projects/ngx-dynamic-json-form/src",
      "prefix": "ndf",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-dynamic-json-form/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-dynamic-json-form/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-dynamic-json-form/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ngx-dynamic-json-form/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        }
      }
    },
    "ngx-dynamic-json-form-material": {
      "projectType": "library",
      "root": "projects/ngx-dynamic-json-form-material",
      "sourceRoot": "projects/ngx-dynamic-json-form-material/src",
      "prefix": "ndf-mat",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-dynamic-json-form-material/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-dynamic-json-form-material/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-dynamic-json-form-material/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ngx-dynamic-json-form-material/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        },
        "storybook": {
          "builder": "@storybook/angular:start-storybook",
          "options": {
            "configDir": "projects/ngx-dynamic-json-form-material/.storybook",
            "browserTarget": "ngx-dynamic-json-form-material:build",
            "compodoc": false,
            "compodocArgs": ["-e", "json", "-d", "projects/ngx-dynamic-json-form-material"],
            "port": 6006,
            "styles": [
              { "input": "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css" },
              { "input": "./node_modules/material-design-icons/iconfont/material-icons.css" },
              { "input": "./projects/ngx-dynamic-json-form-material/.storybook/examples.css" }
            ]
          }
        },
        "build-storybook": {
          "builder": "@storybook/angular:build-storybook",
          "options": {
            "configDir": "projects/ngx-dynamic-json-form-material/.storybook",
            "browserTarget": "ngx-dynamic-json-form-material:build",
            "compodoc": false,
            "compodocArgs": ["-e", "json", "-d", "projects/ngx-dynamic-json-form-material"],
            "outputDir": "dist/storybook/ngx-dynamic-json-form-material"
          }
        }
      }
    },
    "ngx-dynamic-json-form-material-example": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/ngx-dynamic-json-form-material-example",
      "sourceRoot": "projects/ngx-dynamic-json-form-material-example/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ngx-dynamic-json-form-material-example",
            "index": "projects/ngx-dynamic-json-form-material-example/src/index.html",
            "main": "projects/ngx-dynamic-json-form-material-example/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/ngx-dynamic-json-form-material-example/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/ngx-dynamic-json-form-material-example/src/favicon.ico",
              "projects/ngx-dynamic-json-form-material-example/src/assets"
            ],
            "styles": [
              "projects/ngx-dynamic-json-form-material-example/src/styles.scss",
              "node_modules/highlight.js/styles/github.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ngx-dynamic-json-form-material-example:build:production"
            },
            "development": {
              "browserTarget": "ngx-dynamic-json-form-material-example:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ngx-dynamic-json-form-material-example:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/ngx-dynamic-json-form-material-example/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/ngx-dynamic-json-form-material-example/src/favicon.ico",
              "projects/ngx-dynamic-json-form-material-example/src/assets"
            ],
            "styles": ["projects/ngx-dynamic-json-form-material-example/src/styles.scss"],
            "scripts": []
          }
        }
      }
    }
  }
}
