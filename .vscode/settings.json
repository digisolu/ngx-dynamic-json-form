{
    "prettier.printWidth": 100,
    "prettier.singleQuote": true,
    "cSpell.language": "en,de",
    "cSpell.enableFiletypes": [
        "mdx"
    ],
    "cSpell.words": [
      "autodocs",
      "compodoc",
      "devkit",
      "Hanft",
      "iconfont",
      "lcov",
      "lnkd",
      "packagr",
      "sbdocs",
      "stylelint",
      "theming"
    ],
    "eslint.options": {
      "extensions": [".ts", ".html"]
    },
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact",
      "html"
    ],
}
