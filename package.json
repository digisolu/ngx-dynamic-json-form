{
  "name": "@ngx-dynamic-json-form/common",
  "version": "0.9.2",
  "author": "Michael Hanft <21217443+digisolu@users.noreply.github.com>",
  "contributors": [
    "Michael Hanft <21217443+digisolu@users.noreply.github.com>"
  ],
  "keywords": [
    "angular",
    "angular material forms",
    "forms",
    "dynamic forms",
    "ngx-dynamic-form",
    "ngx-dynamic-json-form",
    "angular forms",
    "json forms",
    "dynamic forms",
    "backend driven forms",
    "backend forms",
    "form library"
  ],
  "license": "MIT",
  "description": "NGX Dynamic JSON forms is a library to generate forms for Angular based on JSON.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digisolu/ngx-dynamic-json-form.git"
  },
  "bugs": {
    "url": "https://github.com/digisolu/ngx-dynamic-json-form/issues"
  },
  "homepage": "https://github.com/digisolu/ngx-dynamic-json-form",
  "scripts": {
    "build": "npm run format && npm run test:build && npm run lib:core:build && npm run lib:material:build && npm run lib:material:docs:build",
    "format": "prettier --write projects --cache",
    "lib:core:build": "ng build ngx-dynamic-json-form",
    "lib:core:dev": "ng build ngx-dynamic-json-form --watch --configuration development",
    "lib:core:docs:json": "compodoc -p projects/ngx-dynamic-json-form/tsconfig.lib.json -d docs/ngx-dynamic-json-form -e json",
    "lib:core:test:build": "ng test --project ngx-dynamic-json-form --no-watch --no-progress",
    "lib:core:test:dev": "ng test --project ngx-dynamic-json-form",
    "lib:material:build": "ng build ngx-dynamic-json-form-material",
    "lib:material:dev": "ng build ngx-dynamic-json-form-material --watch --configuration development",
    "lib:material:docs:build": "npm run lib:core:docs:json && npm run lib:material:docs:json && npm run lib:material:storybook:build",
    "lib:material:docs:dev": "npm run lib:core:docs:json && npm run lib:material:docs:json && npm run lib:material:storybook",
    "lib:material:docs:json": "compodoc -p projects/ngx-dynamic-json-form-material/tsconfig.lib.json -d docs/ngx-dynamic-json-form-material -e json",
    "lib:material:storybook:build": "ng run ngx-dynamic-json-form-material:build-storybook",
    "lib:material:storybook:dev": "ng run ngx-dynamic-json-form-material:storybook",
    "lib:material:storybook:server": "http-server dist/storybook/ngx-dynamic-json-form-material -g -o -b",
    "lib:material:test:build": "ng test --project ngx-dynamic-json-form-material --no-watch --no-progress",
    "lib:material:test:dev": "ng test --project ngx-dynamic-json-form-material",
    "ng": "ng",
    "start": "ng serve",
    "test:build": "concurrently \"npm:lib:core:test:build\" \"npm:lib:material:test:build\"",
    "test": "concurrently \"npm:lib:core:test:dev\" \"npm:lib:material:test:dev\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.0.3",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.0.4",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@ngx-dynamic-json-form/core": "file:../dist/ngx-dynamic-json-form",
    "@ngx-dynamic-json-form/material": "file:../dist/ngx-dynamic-json-form-material",
    "install": "^0.13.0",
    "material-design-icons": "^3.0.1",
    "ngx-mat-select-search": "^7.0.1",
    "npm": "^9.6.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.0",
    "@angular/cli": "^15.2.6",
    "@angular/compiler-cli": "^15.2.0",
    "@compodoc/compodoc": "^1.1.19",
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-interactions": "^7.0.7",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/angular": "^7.0.7",
    "@storybook/blocks": "^7.0.7",
    "@storybook/manager-api": "^7.0.7",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/theming": "^7.0.7",
    "@types/jasmine": "~4.3.1",
    "@types/prettier": "^2.7.2",
    "concurrently": "^8.0.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "md5": "^2.3.0",
    "ng-packagr": "^15.0.0",
    "prettier": "2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.3",
    "typescript": "~4.8.2"
  }
}
